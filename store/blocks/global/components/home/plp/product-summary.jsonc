{
 
    "product-summary.shelf#global__product-summary-shelf": {
        "title": "global__product-summary-shelf",
        "children": [
            "flex-layout.row#global__container-product"                       
        ],
        "props": {
            "blockClass": "global__product-summary-shelf"
        }
    },

    "flex-layout.row#global__container-product": {
        "title": "global container product",
        "children": [
            "flex-layout.col#global__col-product--image",
            "flex-layout.col#global__col-product--description-price" 
        ]
    },

    "flex-layout.col#global__col-product--image": {
        "title": "global col image",
        "children": [
            "flex-layout.row#global__row-product--image"
        ]
    },

    "flex-layout.col#global__col-product--description-price": {
        "title": "global col description price",
        "children": [
            "flex-layout.row#global__row-product--description",
            "flex-layout.row#global__row-price"
        ]
    },

    "flex-layout.row#global__row-product--image": {
        "title": "global row image",
        "children": [
            "product-summary-image#global__product-image"
        ],
        "props": {
            "blockClass": "global__row-product--image"
        }
    },

    "product-summary-image#global__product-image": {
        "title":"global product image",
        "props": {
            "src":"/arquivos/product.png",
            "showBadge":true,
            "badgeText": "OFF",
            "displayMode": "normal",
            "placeholder": "undefined",
            "mainImageLabel":"example",
            "blockClass":"global__product-image",
            "widt":{
                "phone":"250px"
            },
            "height":{
                "phone": "205px"
            }    
        }
    },

    "flex-layout.row#global__row-product--description": {
        "title": "global row description",
        "children": [
            "product-summary-description#global__product-description"
        ],
        "props": {
            "blockClass": "global__row-product--description"
        }
    },

    "product-summary-description#global__product-description":{
        "title": "desktop__product-summary-shelf-description",
        "props": {
            "blockClass": "global__product-description",
            "displayMode": "text"
        }
    },

    

    "flex-layout.row#global__row-price": {
        "title": "desktop__price-row",
        "children": [
            "product-list-price#global__product--price",
            "product-selling-price#global__product--selling-price"
        ],
        "props": {
            "blockClass": "global__row-price"
        }
    },

    "product-selling-price#global__product--selling-price": {
        "title": "global selling price",
        "props": {
            "blockClass": "global__product--selling-price"
        }
    },

    "product-list-price#global__product--price":{
        "title": "global product price",
        "props":{
            "blockClass": "global__product--price"
        }
    }

 

   
  
}