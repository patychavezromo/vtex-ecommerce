{
    "flex-layout.row#tablet__flex-layout-row-container-info-card": {
        "title": "tablet__flex-layout-row-container-info-card",
        "children": [
            "flex-layout.col#tablet__flex-layout-col-infocard-content-image-textHeading-textDescription-sell"
        ],
        "props": {
            "blockClass": "tablet__flex-layout-row-container-info-card"
        }
    },
    
    "flex-layout.col#tablet__flex-layout-col-infocard-content-image-textHeading-textDescription-sell": {
        "title": "tablet__flex-layout-col-infocard-content-image-textHeading-textDescription-sell",
        "children": [
            "flex-layout.row#tablet__flex-layout-row-infocard-main-image",
            "flex-layout.row#tablet__flex-layout-row-infocard-textheading-textdescription-and-sell"
        ],
        "props": {
            "blockClass": "tablet__flex-layout-col-infocard-content-image-textHeading-textDescription-sell"
        }
    },

    "flex-layout.row#tablet__flex-layout-row-infocard-main-image": {
        "title": "tablet__flex-layout-row-infocard-main-image",
        "children": [
            "image#tablet__image-infocard"            
        ],
        "props": {
            "blockClass": "tablet__flex-layout-row-infocard-main-image"
        }
    },

    "flex-layout.row#tablet__flex-layout-row-infocard-textheading-textdescription-and-sell": {
        "title": "tablet__flex-layout-row-infocard-textheading-textdescription-and-sell",
        "children": [
            "flex-layout.col#tablet__flex-layout-col-infocard-textheading-and-textDescription",
            "flex-layout.col#tablet__flex-layout-col-infocard-image-sell"
        ],
        "props": {
            "preserveLayoutOnMobile": true,
            "blockClass": "tablet__flex-layout-row-infocard-textheading-textdescription-and-sell"
        }
    },

    "image#tablet__image-infocard": {
        "title": "tablet__image-infocard",
        "props": {
          "src": "/arquivos/Cardelement.jpg",
          "blockClass": "tablet__image-infocard"
        }
    },

    "flex-layout.col#tablet__flex-layout-col-infocard-textheading-and-textDescription": {
        "title": "tablet__flex-layout-col-infocard-textheading-and-textDescription",
        "children": [
            "rich-text#tablet__richtext-infocards-textHeading",
            "rich-text#tablet__richtext-infocards-textdescription"
        ],
        "props": {
            "blockClass": "tablet__flex-layout-col-infocard-textheading-and-textDescription"
        }       
    },

    "flex-layout.col#tablet__flex-layout-col-infocard-image-sell": {
        "title": "tablet__flex-layout-col-infocard-image-sell",
        "children": [
            "image#tablet__image-infocard-image-sell"
        ],
        "props": {
            "width": "20%",
            "verticalAlign": "middle",
            "blockClass": "tablet__flex-layout-col-infocard-image-sell"
        }
    },

    "rich-text#tablet__richtext-infocards-textHeading": {
        "title": "tablet__richtext-infocards-textHeading",
        "props": {
            "textAlignment": "CENTER",
            "text": "Heading",
            "textPosition": "LEFT",
            "blockClass": "tablet__richtext-infocards-textHeading"
        }
    },

    "rich-text#tablet__richtext-infocards-textdescription": {
        "title": "tablet__richtext-infocards-textdescription",
        "props": {
            "textAlignment": "LEFT",
            "text": "Builder platform is made by, buildership platform is made by...",
            "textPosition": "LEFT",
            "blockClass": "tablet__richtext-infocards-textdescription"
        }
    },

    "image#tablet__image-infocard-image-sell": {
        "title": "tablet__image-infocard-image-sell",
        "props": {
            "src": "/arquivos/Artworktrailing.jpg",
            "blockClass": "tablet__image-infocard-image-sell"
        }
    }

}