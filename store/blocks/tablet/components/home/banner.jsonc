{
    "flex-layout.row#tablet__main-slider": {
        "title": "Tablet main home",
        "children": [
            "list-context.image-list#tablet__list-context-image-list-container-main-banner"
        ],
        "props": {
            "blockClass": "tablet-slider-banner"
        }
    },

    "list-context.image-list#tablet__list-context-image-list-container-main-banner": {
        "title": "tablet__list-context-image-list-container-main-banner",
        "children": [
            "slider-layout#tablet__slider-layout-banner"
        ],
        "props": {
          "preload": true,
          "height": 650,
          "images": [
                {
                    "image": "/arquivos/Hero-Desktop-banner.png",
                    "description": "product_watch"
                }
            ],
            "blockClass": "tablet__list-context-image-list-container-main-banner"
        }
    },
    "slider-layout#tablet__slider-layout-banner": {
        "title": "tablet__slider-layout-banner",
        "props": {
            "itemsPerPage": {
                "desktop": 5,
                "tablet": 1,
                "phone": 1
            },
            "blockClass": "tablet__slider-layout-banner"
        }
    }
}