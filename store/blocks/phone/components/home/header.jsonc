    "flex-layout.col#phone__flex-layout-col-logo": {
        "title": "phone__flex-layout-col-logo",
        "props": {
            "blockClass": "phone__flex-layout-col-logo"
            // "verticalAlign":"middle"
        },
        "children": [
            "logo"
        ]
    },

   

    "flex-layout.col#phone__container-overlay-trigger-mobile": {
        "title": "phone__container-overlay-trigger-mobile",
        "children": [
            "overlay-trigger#phone-header__overlay-trigger-mobile"
        ],
        "props": {
            "blockClass": "phone__container-overlay-trigger-mobile",
            "verticalAlign":"middle",
            "horizontalAlign":"center",
            "width": "10%"
        }
    },

    "flex-layout.col#phone__flex-layout-col-minicart.v2": {
        "title": "phone__flex-layout-col-minicart.v2",
        "children": [
            "flex-layout.row#phone__minicart"
        ],
        "props": {
            "blockClass": "phone__flex-layout-col-minicart",
            "verticalAlign":"middle",
            "horizontalAlign":"center",
            "width": "25%"
        }
    },
  
    "flex-layout.col#phone__flex-layout-col-login": {
        "title": "phone__flex-layout-col-login",
        "children": [
            "login#phone-login__button"
        ],
        "props": {
            "blockClass":"phone__flex-layout-col-login",
            "verticalAlign":"middle",
            "horizontalAlign":"center",
            "width": "48%"
        }
    }, 
    
    "login#phone-login__button": {
        "props": {
            "iconLabel":"Login",
            "emailAndPasswordTitle": "LOG-IN",
            "accessCodeTitle": "Acess Code LOG-IN",
            "emailPlaceholder": "e-mail",
            "passwordPlaceholder": "password",
            "showPasswordVerificationIntoTooltip": true,
            "blockClass": "phone-login__button"
        }
    }  
}