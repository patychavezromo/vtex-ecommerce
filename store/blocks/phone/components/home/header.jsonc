{
    "sticky-layout#phone__sticky-layout-header":{
        "title": "phone__sticky-layout-header",
        "props": {
            "blockClass": "phone__sticky-layout-header"
        },
        "children": [
            "flex-layout.row#phone__flex-layout-row-header-container-elements-left-and-right",
            "vtex.menu@2.x:menu#phone__menu-home"
        ]
    },

    "flex-layout.row#phone__flex-layout-row-header-container-elements-left-and-right": {
        "title": "phone__flex-layout-col-header-container-elements-left-and-right",
        "children": [
            "flex-layout.col#phone__flex-layout-col-header-container-elements-left",
            "flex-layout.col#phone__flex-layout-col-header-container-elements-righ"
        ],
        "props": {
            "horizontalAlign": "center",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "preserveLayoutOnMobile":true,
            "blockClass": "phone__flex-layout-row-header-container-elements-left-and-right"            
        }
    },

    "flex-layout.col#phone__flex-layout-col-header-container-elements-left": {
        "title": "phone__flex-layout-col-header-container-elements-left",
        "children": [
            "flex-layout.row#phone__flex-layout-row-header-container-elements-left"
        ],
        "props": {
            "blockClass": "phone__flex-layout-col-header-container-elements-left"
        }
    },

    "flex-layout.col#phone__flex-layout-col-header-container-elements-righ": {
        "title": "phone__flex-layout-col-header-container-elements-right",
        "children": [
            "flex-layout.row#phone__flex-layout-row-header-container-elements-right"
        ],
        "props": {
            "blockClass": "phone__flex-layout-col-header-container-elements-righ"
        }
    },

    "flex-layout.row#phone__flex-layout-row-header-container-elements-left": {
        "title": "phone__flex-layout-row-header-container-elements-left",
        "children": [
            // "flex-layout.col#phone__flex-layout-col-menu-burger",
            "flex-layout.row#phone__row-mega-menu",
            "flex-layout.col#phone__flex-layout-col-logo"
        ],
        "props": {
            "blockClass": "phone__flex-layout-row-header-container-elements-left",
            "preserveLayoutOnMobile":true
        }
    },   

    "flex-layout.row#phone__flex-layout-row-header-container-elements-right": {
        "title": "phone__flex-layout-row-header-container-elements-right",
        "children": [
            "flex-layout.col#phone__container-overlay-trigger-mobile",
            "flex-layout.col#phone__flex-layout-col-minicart.v2",
            "flex-layout.col#phone__flex-layout-col-login"
        ],
        "props": {
            "blockClass": "phone__flex-layout-row-header-container-elements-right",
            "preserveLayoutOnMobile":true
        }
    },

