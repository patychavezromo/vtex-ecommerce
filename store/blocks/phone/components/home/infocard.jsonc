{
    "flex-layout.row#phone__flex-layout-row-container-info-card": {
        "title": "phone__flex-layout-row-container-info-card",
        "children": [
            "flex-layout.col#phone__flex-layout-col-infocard-content-image-textheading-textdesciption-and-button"
        ],
        "props": {
            "blockClass": "phone__flex-layout-row-container-info-card"
        }
    },
    
    "flex-layout.col#phone__flex-layout-col-infocard-content-image-textheading-textdesciption-and-button": {
        "title": "phone__flex-layout-col-infocard-content-image-textheading-textdesciption-and-button",
        "children": [
            "flex-layout.row#phone__flex-layout-row-infocard-image",
            "flex-layout.row#phone__flex-layout-row-infocard-content-textheading-textdescription-and-sell",
            "flex-layout.row#phone__flex-layout-row-container-infocard-linkbutton"
        ],
        "props": {
            "blockClass": "phone__flex-layout-col-infocard-content-image-textheading-textdesciption-and-button"
        }

    },

    "flex-layout.row#phone__flex-layout-row-infocard-image": {
        "title": "phone__flex-layout-row-infocard-image",
        "children": [
            "image#phone__main-image-infocard"          
        ]
    },

    "flex-layout.row#phone__flex-layout-row-infocard-content-textheading-textdescription-and-sell": {
        "title": "phone__flex-layout-row-infocard-content-textheading-textdescription-and-sell",
        "children": [
            "flex-layout.col#phone__flex-layout-col-infocard-container-header-and-text",
            "flex-layout.col#phone__flex-layout-col-infocard-content-sell"
        ],
        "props": {
            "preserveLayoutOnMobile": true,
            "blockClass": "phone__flex-layout-row-infocard-content-textheading-textdescription-and-sell"
        }
    },

    "image#phone__main-image-infocard": {
        "title": "phone__main-image-infocard",
        "props": {
          "src": "/arquivos/Cardelement.jpg",
          "blockClass": "phone__main-image-infocard"
        }
    },

    "flex-layout.col#phone__flex-layout-col-infocard-container-header-and-text": {
        "title": "phone__flex-layout-col-infocard-container-header-and-text",
        "children": [
            "rich-text#phone__richtext-infocards-textheading",
            "rich-text#phone__richtext-infocards-text-description"
        ]        
    },

    "flex-layout.col#phone__flex-layout-col-infocard-content-sell": {
        "title": "phone__flex-layout-col-infocard-content-sell",
        "children": [
            "image#phone__image-sell-infocard"
        ],
        "props": {
            "width": "18%",
            "verticalAlign": "middle",
            "blockClass": "phone__flex-layout-col-infocard-content-sell"
        }
    },

    "rich-text#phone__richtext-infocards-textheading": {
        "title": "phone__richtext-infocards-textheading",
        "props": {
            "textAlignment": "CENTER",
            "text": "Heading",
            "textPosition": "LEFT",
            "blockClass": "phone__richtext-infocards-textheading"
        }
    },

    "rich-text#phone__richtext-infocards-text-description": {
        "title": "phone__richtext-infocards-text-description",
        "props": {
            "textAlignment": "LEFT",
            "font": "heading-3",
            "text": "Builder platform is made by...",
            "textPosition": "LEFT",
            "blockClass": "phone__richtext-infocards-text-description"
        }
    },

    "image#phone__image-sell-infocard": {
        "title": "phone image sell",
        "props": {
            "src": "/arquivos/Artworktrailing.jpg"
        }
    },

    "flex-layout.row#phone__flex-layout-row-container-infocard-linkbutton":{
        "title": "phone__flex-layout-row-container-infocard-linkbutton",
        "children": [
            "image#phone__image-infocard-buttonlink"
        ],
        "props": {
            "blockClass": "phone__flex-layout-row-container-infocard-linkbutton"
        }
    },

   

    "image#phone__image-infocard-buttonlink": {
        "title": "phone__image-infocard-buttonlink",
        "props": {
            "src": "/arquivos/Button_infocard.png",
            "maxHeight": 55,
            "blockClass": "phone__image-infocard-buttonlink"
        }
    }

}

