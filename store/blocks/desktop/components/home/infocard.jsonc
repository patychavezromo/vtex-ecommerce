{
    "flex-layout.row#desktop-flex-layout-row-container-info-card": {
        "title": "desktop-flex-layout-row-container-info-card",
        "children": [
            "flex-layout.col#desktop-flex-layout-col-infocard-container-image-text-sell-and button"
        ],
        "props": {
            "blockClass": "desktop-flex-layout-row-container-info-card"
        }
    },
    
    "flex-layout.col#desktop-flex-layout-col-infocard-container-image-text-sell-and button": {
        "title": "desktop-flex-layout-col-infocard-container-image-text-sell-and button",
        "children": [
            "flex-layout.row#desktop__flex-layout-row-infocard-image",
            "flex-layout.row#desktop__flex-layout-row-infocard-content-heading-text-and-sell",
            "flex-layout.row#desktop__flex-layout-row-infocard-container-button"
        ]

    },

    "flex-layout.row#desktop__flex-layout-row-infocard-image": {
        "title": "desktop__flex-layout-row-infocard-image",
        "children": [
            "image#desktop__infocard-sell-image"            
        ]
    },

    "flex-layout.row#desktop__flex-layout-row-infocard-content-heading-text-and-sell": {
        "title": "desktop__flex-layout-row-infocard-content-heading-text-and-sell",
        "children": [
            "flex-layout.col#desktop__flex-layout-col-infocard-content-textheading-and-textdescription",
            "flex-layout.col#desktop__flex-layout-col-infocard-container-sell"
        ],
        "props": {
            "preserveLayoutOnMobile": true
        }
    },

    "image#desktop__infocard-sell-image": {
        "title": "desktop__infocard-sell-image",
        "props": {
          "src": "/arquivos/Cardelement.jpg",
          "blockClass": "desktop__infocard-sell-image"
        }
    },

    "flex-layout.col#desktop__flex-layout-col-infocard-content-textheading-and-textdescription": {
        "title": "desktop__flex-layout-col-infocard-content-textheading-and-textdescription",
        "children": [
            "rich-text#desktop__richtext-infocards-text-heading",
            "rich-text#desktop__richtext-infocards-text-description"
        ],
        "props": {
            "blockClass": "desktop__flex-layout-col-infocard-content-textheading-and-textdescription"
        }     
    },

    "flex-layout.col#desktop__flex-layout-col-infocard-container-sell": {
        "title": "desktop__flex-layout-col-infocard-container-sell",
        "children": [
            "image#desktop__image-sell-infocard"
        ],
        "props": {
            "width": "20%",
            "verticalAlign": "middle",
            "blockClass":"Desktop-infocard-sell-image"
        }
    },

    "rich-text#desktop__richtext-infocards-text-heading": {
        "title": "desktop__richtext-infocards-text-heading",
        "props": {
            "textAlignment": "CENTER",
            "text": "Heading",
            "textPosition": "LEFT",
            "blockClass": "desktop__richtext-infocards-text-heading"
        }
    },

    "rich-text#desktop__richtext-infocards-text-description": {
        "title": "desktop__richtext-infocards-text-description",
        "props": {
            "textAlignment": "LEFT",
            "font": "fast",
            "text": "Builder platform is made by, buildership platform is made by...",
            "textPosition": "LEFT",
            "blockClass": "desktop__richtext-infocards-text-description"
        }
    },

    "image#desktop__image-sell-infocard": {
        "title": "desktop__image-sell-infocard",
        "props": {
            "src": "/arquivos/Artworktrailing.jpg",
            "blockClass": "Desktop-infocard-sell-image"
        }
    },

    "flex-layout.row#desktop__flex-layout-row-infocard-container-button":{
        "title": "desktop__flex-layout-row-infocard-container-button",
        "children": [
            "image#desktop__image-buttonlink-infocard"
        ],
        "props": {
            "blockClass": "desktop__flex-layout-row-infocard-container-button"
        }
    },

   

    "image#desktop__image-buttonlink-infocard": {
        "props": {
            "src": "/arquivos/Button_infocard.png",
            "maxHeight": 55,
            "blockClass": "desktop__image-buttonlink-infocard"
        }
    }

}