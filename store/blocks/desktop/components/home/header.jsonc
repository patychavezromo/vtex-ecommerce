{
    "sticky-layout#desktop": {
        "props": {
            "blockClass": "sticky-header-Desktop"
        },
        "children": [
            "flex-layout.row#desktop",
            "vtex.menu@2.x:menu#desktop__menu-home"
        ]
    },
    "flex-layout.row#desktop": {
        "props": {
            // "horizontalAlign": "center",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "blockClass": "desktop-container-menu-up"
        },
        "children": [
            "flex-layout.col#logo-desktop",
            // "flex-layout.col#spacer",
            "flex-layout.col#search-Bar-Desktop",
            "flex-layout.col#Desktop-layoutcol-allLinks",
            "flex-layout.col#minicart",
            "flex-layout.col#loginDesktop"
            // "locale-switcher",
            // "minicart.v2",
            // "vtex.menu@2.x:menu#websites#desktop__menu-home"           
        ]
    },
    "flex-layout.col#logo-desktop": {
        "props": {
            "verticalAlign": "top",
            "blockClass": "Desktop-logo"
        },
        "children": [
            "logo"
        ]
    },
    "flex-layout.col#loginDesktop": {
        "props": {
            // "verticalAlign": "middle",
            "blockClass":"Desktop-layout-login"
        },
        "children": [
            "login"
        ]
    },

    "flex-layout.col#search-Bar-Desktop": {
        "props": {
            "verticalAlign": "top",
            "blockClass": "Desktop-layout-Search-Bar"
        },
        "children": [
            "search-bar"
        ]
    },

    "flex-layout.col#minicart": {
        "children": [
            "minicart.v2"
        ]
    },

    "flex-layout.col#Desktop-layoutcol-allLinks": {
        "children": [
            "flex-layout.row#Desktop-container-Links"
        ]
    },

    "flex-layout.row#Desktop-container-Links": {
        "children": [
           "flex-layout.col#Link-1-home",
           "flex-layout.col#Link-2-home",
           "flex-layout.col#Link-3-home",
           "flex-layout.col#Link-4-home",
           "flex-layout.col#Link-5-home"
        ],
        "props": {
            "blockClass": "Desktop-container-Links"
        }
    },

    "flex-layout.col#Link-1-home":{
        "children":[
            "rich-text#Tendencias"
        ],
        "props": {
            "blockClass":"linksDesktop"
        }
    },
    "flex-layout.col#Link-2-home":{
